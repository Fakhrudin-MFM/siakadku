<section class="content-header">
  <h1>Data Mahasiswa</h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>        
    <li class="active">Dashboard</li>
  </ol>
</section>
<!-- Main content-->
<section class="content">
  <!-- Your Page Content Here-->
  <!-- /.row-->
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">Halo dunia</h3>
        </div>
        <!-- /.box-header-->
        <div class="box-body">
          <p>Welcome to {{title}}</p>
{{#with matkul}}
            <form class="form-horizontal" action="/matakuliah/edit/{{id}}?_method=PUT" method="POST">
              <div class="form-group">
                <label for="nidn" class="col-sm-3 control-label">Id</label>
                <div class="col-sm-7">
                  <input type="text" class="form-control" name="id" value="{{id}}">
                </div>
              </div>

              <div class="form-group">
                <label for="nama" class="col-sm-3 control-label">Nama Mata Kuliah</label>
                <div class="col-sm-7">
                  <input type="text" class="form-control" name="nama_matkul" value="{{nama_matkul}}">
                </div>
              </div>

              <div class="form-group">
                <label for="nama" class="col-sm-3 control-label">SKS</label>
                <div class="col-sm-7">
                  <input type="number" class="form-control" name="sks" value="{{sks}}">
                </div>
              </div>

              <div class="form-group">
                <div class="col-sm-10">
                  <input type="submit" name="save" class="btn btn-primary pull-right" >
                </div>
              </div>
            </form>
{{/with}}
        </div>
        <!-- /.box-body-->
      </div>
      <!-- /.box-->
    </div>
  </div>
</section>